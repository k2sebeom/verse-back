// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  MUSICIAN
  AUDIENCE
}

model User {
  id     Int @id @default(autoincrement())
  museId Int @unique

  points    Int    @default(500)
  streamKey String
  liveUrl   String
}
